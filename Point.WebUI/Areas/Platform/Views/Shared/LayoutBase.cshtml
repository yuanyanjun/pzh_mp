<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
@RenderSection("PageBefore", false)
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="Content-Language" content="utf-8" />
    <meta name="robots" content="NOINDEX,NOFOLLOW" />
    @{

        var t = Request.QueryString["windowtitle"];
        if (String.IsNullOrWhiteSpace(t))
        {
            t = ViewBag.Title;
        }

    }
    <title>@t</title>
    @RenderSection("Style", false)

</head>
<body>
    @RenderSection("Body", false)

    <script type="text/javascript" src="~/Resources/platform/js/jquery-1.8.1.min.js"></script>
    <script type="text/javascript" src="~/Resources/common/js/vue2.1.10.js"></script>
    <script type="text/javascript" src="~/Resources/common/js/vue-extend.js"></script>
    <script type="text/javascript">
        $G = {
            root:'@Point.Common.AppSetting.Default.GetItem("CurrentWebBaseUrl")'
        }

        function buildUrl(path){
            if(path)
                return path.replace('~/',$G.root+'/');
            return '';
        }
    </script>
    @RenderSection("Script", false)
</body>
@RenderSection("BodyAfter", false)
</html>
@RenderSection("PageAfter", false)