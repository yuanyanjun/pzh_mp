<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
@RenderSection("PageBefore", false)
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="Content-Language" content="utf-8" />
    <meta name="robots" content="NOINDEX,NOFOLLOW" />
    @{

        var t = Request.QueryString["windowtitle"];
        if (String.IsNullOrWhiteSpace(t))
        {
            t = ViewBag.Title;
        }
        
    }
    <title>@t</title>
    <link rel="stylesheet" type="text/css" href="~/Resources/platform/lib/bootstrap/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="~/Resources/platform/stylesheets/theme.css">
    <link rel="stylesheet" href="~/Resources/platform/lib/font-awesome/css/font-awesome.css">
    @RenderSection("Style", false)
</head>
<body>
    @RenderSection("Body", false)
  <script type="text/javascript" src="~/Resources/platform/lib/jquery-1.7.2.min.js"></script>
  
    @RenderSection("Script", false)
</body>
@RenderSection("BodyAfter", false)
</html>
@RenderSection("PageAfter", false)