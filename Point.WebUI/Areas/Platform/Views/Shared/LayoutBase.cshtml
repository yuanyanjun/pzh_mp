<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
@RenderSection("PageBefore", false)
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="Content-Language" content="utf-8" />
    <meta name="robots" content="NOINDEX,NOFOLLOW" />
    @{

        var t = Request.QueryString["windowtitle"];
        if (String.IsNullOrWhiteSpace(t))
        {
            t = ViewBag.Title;
        }
        
    }
    <title>@t</title>
    <link href="~/Resources/platform/jquery-easyui-1.5.2/themes/default/easyui.css" rel="stylesheet" />
    <link href="~/Resources/platform/jquery-easyui-1.5.2/themes/icon.css" rel="stylesheet" />
    @RenderSection("Style", false)
   
</head>
<body class="easyui-layout" style="overflow-y: hidden" scroll="no">
    @RenderSection("Body", false)

    <script type="text/javascript" src="~/Resources/platform/js/jquery-1.8.1.min.js"></script>
    <script type="text/javascript" src="~/Resources/platform/js/common.js"></script>
    <script type="text/javascript" src="~/Resources/platform/jquery-easyui-1.5.2/jquery.easyui.min.js"></script>
    @RenderSection("Script", false)
</body>
@RenderSection("BodyAfter", false)
</html>
@RenderSection("PageAfter", false)